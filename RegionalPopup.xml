<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    android:layout_weight="1"
    tools:context=".RegionalPopup">

    <Button
        android:id="@+id/Btn_FirstPrintArea"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:text="첫번째 출력 영역"
        android:fontFamily="@font/nanumgothicbold"
        android:textSize="20sp"
        android:background="@drawable/buttonround"
        android:textColor="@color/white"
        />

    <Button
        android:id="@+id/Btn_SecondPrintArea"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:text="두번째 출력 영역"
        android:fontFamily="@font/nanumgothicbold"
        android:textSize="20sp"
        android:background="@drawable/buttonround"
        android:textColor="@color/white"
        android:layout_marginTop="10dp"
        />

    <Button
        android:id="@+id/Btn_ThirdPrintArea"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:text="세번째 출력 영역"
        android:fontFamily="@font/nanumgothicbold"
        android:textSize="20sp"
        android:background="@drawable/buttonround"
        android:textColor="@color/white"
        android:layout_marginTop="10dp"
        />

    <Button
        android:id="@+id/Btn_FourthPrintArea"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:text="네번째 출력 영역"
        android:fontFamily="@font/nanumgothicbold"
        android:textSize="20sp"
        android:background="@drawable/buttonround"
        android:textColor="@color/white"
        android:layout_marginTop="10dp"
        />

    <Button
        android:id="@+id/Btn_FifthPrintArea"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:text="다섯번째 출력 영역"
        android:fontFamily="@font/nanumgothicbold"
        android:textSize="20sp"
        android:background="@drawable/buttonround"
        android:textColor="@color/white"
        android:layout_marginTop="10dp"
        />

</LinearLayout>
